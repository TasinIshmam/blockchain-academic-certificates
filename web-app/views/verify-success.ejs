<div class="container justify-content-center"  >
    <br>
    <h1 class="text-success text-center">Verification Successful</h1>

    <div class="row cert-container card">
            <div class="col-sm-12 card-body ">
                <div class="row text-center upper-row">
                    <div class="col-sm-4 orange"> <strong>Name <br></strong> <%= certData.studentName %> </div>
                    <div class="col-sm-4 orange"><strong>Email <br></strong> <%= certData.studentEmail %></div>
                    <div class="col-sm-4 orange"> <strong>Date Of Issuing <br></strong> <%= certData.dateOfIssuing %></div>
                </div>
                <div class="row text-center middle-row">
                    <div class="col-sm-4 orange"> <strong>University Name<br></strong> <%= certData.universityName %></div>
                    <div class="col-sm-4 orange"><strong>University Email<br></strong> <%= certData.universityEmail %> </div>
                    <div class="col-sm-4 orange"><strong>Certificate UUID <br></strong><%= certData._id %></div>
                </div>
                <div class="row text-center bottom-row">
                    <% Object.keys(proofData).forEach(function (key) {  %>
                    <div class="col-sm-4 orange"><strong><%= key %><br></strong> <%= proofData[key] %> </div>

                    <% });%>

                </div>
            </div>
        </div>




